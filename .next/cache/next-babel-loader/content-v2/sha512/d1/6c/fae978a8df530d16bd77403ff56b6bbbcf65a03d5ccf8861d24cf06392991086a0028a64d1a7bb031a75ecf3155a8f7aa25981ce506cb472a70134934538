n    return function (dispatch, getState) {\n      var oldTodos = getState().Todos.todos;\n      var todos = [];\n      oldTodos.forEach(function (todo) {\n        if (!todo.completed) {\n          todos.push(todo);\n        }\n      });\n      dispatch({\n        type: todoActions.CHANGE_TODO,\n        todos: todos\n      });\n    };\n  }\n};\nexport default todoActions;","map":null,"metadata":{},"sourceType":"module"}